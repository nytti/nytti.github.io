{
  "name": "new-york-times",
  "version": "0.0.1",
  "description": "New York Times topics frontend app",
  "main": "index.js",
  "scripts": {
    "start": "yarn run dev",
    "lint": "eslint src",
    "test": "jest",
    "test-e2e": "cypress run",
    "test-e2e-open": "cypress open",
    "dev": "webpack-dev-server --mode development --progress --open --config __development/webpack.config.js",
    "build": "webpack --mode production -p --config __production/webpack.config.js",
    "testUpdateSnapshots": "jest --updateSnapshot",
    "travis": "(http-server -p 5001 ./build&); npm run test-e2e"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "./test/adapter/config.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "cypress"
    ],
    "modulePaths": [
      "test/mocks",
      "test/data",
      "src",
      "config"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^4.0.0",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "cypress": "^3.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "react-hot-loader": "^4.3.11",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
